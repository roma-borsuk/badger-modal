{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ESNext",
    "sourceMap": true,
    "declaration": true,
    "noImplicitThis": false,
    "strict": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "esModuleInterop": true,
    "noEmit": true,
    "skipLibCheck": true
  }
}
